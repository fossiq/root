{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "kql",
      "camelcase": "Kql",
      "title": "KQL Parser",
      "scope": "source.kql",
      "file-types": [
        "kql"
      ],
      "injection-regex": "^kql$",
      "class-name": "TreeSitterKql"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "A parser for the Kusto Query Language",
    "authors": [
      {
        "name": "Sushruth Sastry",
        "email": "sushruth.sastry@outlook.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-kql"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}