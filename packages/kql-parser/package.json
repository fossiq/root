{
  "name": "@fossiq/kql-parser",
  "version": "1.0.2",
  "description": "A TypeScript parser for Kusto Query Language (KQL) built with tree-sitter",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "grammar.js",
    "binding.gyp",
    "README.md",
    "tree-sitter-kql.wasm",
    "prebuilds"
  ],
  "keywords": [
    "kql",
    "kusto",
    "parser",
    "tree-sitter",
    "azure",
    "data-explorer",
    "query-language",
    "ast"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fossiq/root.git",
    "directory": "packages/kql-parser"
  },
  "bugs": {
    "url": "https://github.com/fossiq/root/issues"
  },
  "homepage": "https://github.com/fossiq/root/tree/main/packages/kql-parser#readme",
  "author": "Sushruth Sastry",
  "license": "MIT",
  "engines": {
    "bun": ">=1.3.0",
    "node": ">=24.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": false,
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "compile-grammar": "bun src/grammar/compile.ts",
    "generate": "bun run compile-grammar && bunx tree-sitter-cli generate",
    "build:binding": "bun scripts/build-binding.ts",
    "build:wasm": "bunx tree-sitter build --wasm",
    "build": "bun run generate && bun run build:binding && bun run build:wasm && tsc",
    "prebuild": "bun run generate",
    "prepublishOnly": "bun run build && bun run test",
    "test": "bun test tests",
    "test:watch": "bun test tests --watch"
  },
  "dependencies": {
    "tree-sitter": "^0.25.0",
    "tree-sitter-cli": "^0.25.10",
    "web-tree-sitter": "^0.22.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.4",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
