{
  "name": "@fossiq/kql-to-duckdb",
  "version": "1.1.1",
  "description": "Translator from KQL to DuckDB SQL",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "bun test tests",
    "test:coverage": "bun test tests --coverage",
    "ci:publish": "bunx npm@latest publish --ignore-scripts --provenance"
  },
  "dependencies": {
    "@fossiq/kql-parser": "workspace:*",
    "web-tree-sitter": "catalog:"
  },
  "devDependencies": {
    "@types/bun": "catalog:",
    "@types/node": "catalog:",
    "typescript": "catalog:"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}
